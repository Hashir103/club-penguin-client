{
  "name": "clubpenguin",
  "version": "1.0.0",
  "description": "Club Penguin Client",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder build --win --publish never",
    "build-mac": "electron-builder build --mac --publish never",
    "appveyor-build": "electron-builder build --win --publish onTagOrDraft"
  },
  "author": "Club Penguin",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "6.1.2",
    "electron-builder": "22.2.0",
    "electron-is-dev": "1.1.0"
  },
  "repository": "https://github.com/Hashir103/club-penguin-client",
  "dependencies": {
    "discord-rpc": "3.1.0",
    "electron-is-dev": "1.1.0",
    "electron-updater": "4.2.0"
  },
  "build": {
    "appId": "com.clubpenguin.desktop",
    "productName": "Club Penguin",
    "asar": "false",
    "win": {
      "publish": "github"
    }
  }
}
